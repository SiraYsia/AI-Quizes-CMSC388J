{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/quiz_app/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.py"
      }
    ]
  }